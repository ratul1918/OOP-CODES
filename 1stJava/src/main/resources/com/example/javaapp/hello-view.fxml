<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="723.0" prefWidth="573.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javaapp.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <DialogPane />
   <GridPane>
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
   </GridPane>
    <Button onAction="#onHelloButtonClick" text="Hello!" />
   <ButtonBar prefHeight="13.0" prefWidth="52.0" />
 <Button fx:id="button_ClickMe" mnemonicParsing="false" onAction="#onHelloButtonClick" text="Button" />

    <Label onDragDetected="#onHelloButtonClick" prefHeight="29.0" prefWidth="55.0" text="Click Me" textFill="#9a2c2c" wrapText="true">
      <font>
         <Font size="14.0" />
      </font></Label>
</VBox>
